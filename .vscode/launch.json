{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug",
      "url": "http://localhost:8080",
      "userDataDir": "${workspaceRoot}/.vscode/chrome",
      "webRoot": "${workspaceRoot}",
      "smartStep": true,
      "skipFiles": ["node_modules/**"],
      "sourceMapPathOverrides": { "webpack:///*": "${webRoot}/*" }
    }
  ]
}
